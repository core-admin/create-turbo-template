{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}